{
  "name": "funnel-fill-chrome-extension",
  "version": "1.0.0",
  "author": "Oleksandr Reznichenko",
  "description": "Extension for automate data input into the Yuc Funnel",
  "scripts": {
    "ng": "ng",
    "build:web-panel": "ng build web-panel && node elements-build.js",
    "build:web-panel-production": "ng build web-panel --prod && node elements-build.js",
    "start:web-panel": "ng serve web-panel --port 4300",
    "watch:web-panel": "ng build web-panel --watch --delete-output-path false && node elements-build.js",
    "build:chrome": "webpack --config chrome/webpack.config.js",
    "build:chrome-production": "webpack -p --config chrome/webpack.config.prod.js",
    "watch:chrome": "webpack --config chrome/webpack.config.js -w",
    "build": "npm run clean && npm run build:chrome",
    "build:production": "npm run build:chrome-production && npm run build:web-panel-production",
    "watch": "npm run clean && npm-run-all --parallel watch:*",
    "clean": "rimraf ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/elements": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "codice-fiscale-js": "^2.2.1",
    "core-js": "^2.5.4",
    "document-register-element": "1.13.2",
    "redux": "^4.0.1",
    "replace-in-file": "^4.1.0",
    "rxjs": "^6.5.2",
    "rxjs-tslint": "^0.1.7",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@types/chrome": "0.0.81",
    "@types/node": "~8.9.4",
    "awesome-typescript-loader": "^5.2.1",
    "bestzip": "^2.1.2",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "fs-extra": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "tar": ">=4.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
